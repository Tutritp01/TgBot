<!DOCTYPE web-app PUBLIC
        "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
        "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
    <servlet>
        <servlet-name>AllCars</servlet-name>
        <servlet-class>com.tutrit.stoservice.servlets.CarTableServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>AllCars</servlet-name>
        <url-pattern>/carTable</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>CarForm</servlet-name>
        <servlet-class>com.tutrit.stoservice.servlets.CarFormServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>CarForm</servlet-name>
        <url-pattern>/carForm</url-pattern>
    </servlet-mapping>
</web-app>
